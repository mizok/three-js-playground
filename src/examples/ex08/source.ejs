<% const fs = require('fs') %>

<% fs.readdirSync('./src/assets/images/door-wood').forEach(textureName => { %>
  <img src="~@img/door-wood/<%= textureName %>">
  <% }) %>

<% fs.readdirSync('./src/assets/images/gradients').forEach(textureName => { %>
  <img src="~@img/gradients/<%= textureName %>">
<% }) %>

